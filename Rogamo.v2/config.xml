<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget defaultlocale="en-US" id="io.cordova.myappe2785d518e9f4f6ea03055878dd7a400" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
  <name>Rogamo</name>
  <description>
      A Rogamo Ionic Framework and Cordova project.
  </description>
